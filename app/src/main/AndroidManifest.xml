<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.dudu9999.com.qualprova">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".Fragmentos.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Telas.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Telas.CadastroActivity"/>
        <activity android:name=".Telas.PerfilActivity"/>
        <activity
            android:name=".Telas.TelaInicioActivity"
            android:label="@string/title_activity_tela_inicio"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Telas.AdcionaActivity"/>
        <activity android:name=".Telas.AboutActivity">
        </activity>
    </application>

</manifest>